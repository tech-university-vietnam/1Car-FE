{
  "plugins": ["testing-library", "jest-dom"],
  "extends": [
    "plugin:testing-library/react",
    "plugin:jest-dom/recommended",
    "prettier"
  ],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-warning-comments": "warn"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
