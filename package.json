{
  "name": "1-car",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@auth0/auth0-react": "^1.10.2",
    "@cypress/instrument-cra": "^1.4.0",
    "@googlemaps/react-wrapper": "^1.1.35",
    "@reduxjs/toolkit": "^1.8.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.184",
    "@types/node": "^16.11.49",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "antd": "^4.22.6",
    "axios": "^0.27.2",
    "eslint": "8.22.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-testing-library": "^5.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-places-autocomplete": "^3.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "6",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "react-scripts test && cypress run",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "test:jest-cov": "react-scripts test --watchAll=false --coverage --coverageDirectory coverage/jest --coverageThreshold {}",
    "test:cypress-cov": "cypress run  && npx nyc report --report-dir coverage/cypress --reporter=json",
    "test:merge-cov": "npx istanbul-merge --out coverage/combined-coverage.json ./coverage/cypress/coverage-final.json ./coverage/jest/coverage-final.json && npx istanbul report --include coverage/combined-coverage.json --dir coverage text",
    "test:cov": "npm run test:cypress-cov && npm run test:jest-cov && npm run test:merge-cov",
    "lint": "eslint src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@types/cypress": "^1.1.3",
    "@types/google-map-react": "^2.1.7",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.7.0",
    "eslint-config-prettier": "^8.5.0",
    "google-map-react": "^2.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3",
    "universal-cookie": "^4.0.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 10
      }
    }
  }
}
